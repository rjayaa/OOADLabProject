package model;

import state.IState;

public class Tugas {
	// kalau ada keterangan fase, maka guanakan state pattern
	private String namaTugas;
	private IState statusTugas;
	public Tugas(String namaTugas) {
		super();
		this.namaTugas = namaTugas;
	}

	public String getNamaTugas() {
		return namaTugas;
	}

	public void setNamaTugas(String namaTugas) {
		this.namaTugas = namaTugas;
	}
	
	
	
}
